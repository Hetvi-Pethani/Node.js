        background-clip: padding-box;
        box-shadow: 0 0 2px 0 rgba(0, 0, 0, 05);
        border-radius: .25rem;
    }

    .dropdown-item:hover {
        color: #1e2125;
        background-color: #fff;
    }

    .dropdown-menu .dropdown-item:hover {
        color: #f2a100;
        transition: 0.5s;
    }

    .header-icon i {
        margin: 0 10px;
    }

    .header-icon a {
        color: #000;
    }

    /* header end */

