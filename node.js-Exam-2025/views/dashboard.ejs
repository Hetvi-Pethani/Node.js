    }

    .dropdown-toggle::after {
        content: none;
    }

    .dropdown-item:active {
        color: #000;
        text-decoration: none;
        background-color: #fff;
    }

    .dropdown-item {
        width: 200px;
        height: 40px;
        padding: .25rem 1rem;
        clear: both;
        font-weight: 400;
        color: #212529;
        font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
    }

    .dropdown-item:active {
        background-color: none;
    }

    .dropdown-menu {
